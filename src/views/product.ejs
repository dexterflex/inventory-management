<% products.forEach(product=>{ %>
<tr>
    <td><%= product.id %></td>
    <td><%= product.title %></td>
    <td><%= product.desc %></td>
    <td>$ <%= product.price %></td>
    <td><img src="<%= product.imageUrl %>" alt=""></td>
    <td><button class="btn btn-warning my-2">Edit</button>
        <button class="btn btn-danger">Delete</button>
    </td>
</tr>
<% }) %>